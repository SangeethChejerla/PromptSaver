{
    "manifest_version": 3,
    "name": "PromptSaver",
    "version": "1.0",
    "description": "Save and prioritize prompts with drag-and-drop.",
    "permissions": ["storage", "activeTab"],
    "action": {
      "default_popup": "popup.html",
      "default_icon": "logo.png"
    },
    "icons": {
      "128": "logo.png"
    }
  }